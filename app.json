{
  "addons": [
    "redistogo"
  ],
  "description": "Sam's bot for heroku",
  "env": {
    "BUILDPACK_URL": {
      "description": "Only change this if you are sure you need to.",
      "required": true,
      "value": "https://github.com/heroku/heroku-buildpack-nodejs"
    },
    "HUBOT_HEROKU_KEEPALIVE_URL": {
      "description": "Set to use the subdomain of this bot to keep it awake",
      "required": false,
      "value": "https://YOUR_BOT_APP.herokuapp.com"
    },
    "HUBOT_LOG_LEVEL": {
      "description": "Set to `debug` to enable detailed debug logging.",
      "required": false
    },
    "HUBOT_SLACK_TOKEN": {
      "description": "slack Token",
      "required": true
    }
  },
  "keywords": [
    "hubot",
    "bot",
    "chat",
    "cah",
    "heroku",
    "slack"
  ],
  "logo": "https://hubot.github.com/assets/images/layout/hubot-avatar@2x.png",
  "name": "Sam's Hubot with HipChat",
  "repository": "https://github.com/scboucher/sambot"
}
